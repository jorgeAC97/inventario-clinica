<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.clinica.Controladores.PanelLocalizadorController" alignment="CENTER" spacing="15.0">
    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0" left="20.0" />
    </padding>
    <Label text="Localizador de Productos" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
    <TextField fx:id="txtBuscar" promptText="Buscar producto..." />
    <Button fx:id="btnBuscar" text="Buscar" />
</VBox> 